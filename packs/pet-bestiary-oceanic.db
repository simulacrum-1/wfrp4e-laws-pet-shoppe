{"_id":"quSHlE6cC5EhS95X","name":"Squid, Giant","type":"creature","img":"modules/wfrp4e-core/art/bestiary/bog-octopus.webp","data":{"characteristics":{"ws":{"type":"number","label":"Weapon Skill","abrev":"WS","initial":55,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":35,"bonus":3,"cost":25},"bs":{"type":"Number","label":"Ballistic Skill","abrev":"BS","initial":0,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":0,"bonus":0,"cost":25},"s":{"type":"Number","label":"Strength","abrev":"S","initial":80,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":80,"bonus":8,"cost":25},"t":{"type":"Number","label":"Toughness","abrev":"T","initial":75,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":75,"bonus":7,"cost":25},"i":{"type":"Number","label":"Initiative","abrev":"I","initial":15,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":15,"bonus":1,"cost":25},"ag":{"type":"Number","label":"Agility","abrev":"Ag","initial":45,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":55,"bonus":5,"cost":25},"dex":{"type":"Number","label":"Dexterity","abrev":"Dex","initial":0,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":0,"bonus":0,"cost":25},"int":{"type":"Number","label":"Intellegence","abrev":"Int","initial":15,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":5,"bonus":0,"cost":25},"wp":{"type":"Number","label":"Willpower","abrev":"WP","initial":55,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":65,"bonus":6,"cost":25},"fel":{"type":"Number","label":"Fellowship","abrev":"Fel","initial":0,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":0,"bonus":0,"cost":25}},"status":{"wounds":{"type":"Number","label":"Wounds","abrev":"W","value":108,"max":108},"advantage":{"type":"Number","label":"Advantage","value":0,"max":10},"criticalWounds":{"type":"Number","label":"Critical Wounds","value":0,"max":7,"descriptions":""},"sin":{"type":"Number","label":"Sin","va